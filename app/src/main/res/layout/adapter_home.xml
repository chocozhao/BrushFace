<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:foreground="@drawable/selector_foreground">

    <LinearLayout
        android:id="@+id/home_bg_ll"
        android:layout_width="80dp"
        android:layout_height="86dp"
        android:background="@color/white"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/home_item_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/home_adapter_guide" />

        <TextView
            android:id="@+id/home_item_tv"
            style="@style/text_normal_color_6"
            android:paddingBottom="10dp"
            android:text="极速收款"
            android:textSize="@dimen/text_sub"
            android:visibility="visible" />
    </LinearLayout>
</LinearLayout>
